Private Sub cmdDo_Click()
    Dim infos As String
    txtMessage.Value = "ELABORAZIONE IN CORSO ..."
    ok = modScenario_GetAllocation(lblFilename.Caption, chkDryRun.Value, infos)
    If ok Then
        txtMessage.Value = infos
    Else
        txtMessage.Value = "ELABORAZIONE TERMINATA IN ERRORE"
    End If
    ' cmdDo.Enabled = False
