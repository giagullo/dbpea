End Sub

Private Sub Comando3_Click()
    Dim sFile As String
    With Application.FileDialog(msoFileDialogOpen)
        .AllowMultiSelect = False
        .Filters.Clear
        .Filters.Add "File excel", "*.xls*"
        .show
        If .SelectedItems.Count = 0 Then
            Exit Sub
        End If
        sFile = .SelectedItems.Item(1)
    End With

    lblFilename.Caption = sFile
    cmdDo.Enabled = True
End Sub

